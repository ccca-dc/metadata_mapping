    	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    	<skos:inScheme rdf:resource="http://registry.inspire.gv.at/codelist/LISALandCoverClassValue"/>
	    <skos:prefLabel xml:lang="en">mixed trees</skos:prefLabel>
 	   	<skos:prefLabel xml:lang="de">Mischung Laub/Nadel</skos:prefLabel>
