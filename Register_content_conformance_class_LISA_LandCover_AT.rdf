  	<rdf:Description rdf:about="http://registry.inspire.gv.at/codelist/LISALandCoverClassValue/2_4_9_3_ConiferousTrees">
    	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    	<skos:inScheme rdf:resource="http://registry.inspire.gv.at/codelist/LISALandCoverClassValue"/>
 	    <skos:prefLabel xml:lang="en">coniferous trees</skos:prefLabel>
    	<skos:prefLabel xml:lang="de">Nadelbäume</skos:prefLabel>
  		<skos:definition xml:lang="en">no definition availabe yet</skos:definition>
